{
  "name": "@tofandel/vue-unlayer",
  "private": false,
  "version": "1.0.0",
  "main": "./dist/vue-unlayer.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve demo/main.js",
    "demo:build": "vue-cli-service build demo/main.js",
    "build": "vue-cli-service build --target lib --name vue-unlayer ./src/EmailEditor.vue && rm dist/demo.html",
    "lint": "vue-cli-service lint .",
    "lint:fix": "vue-cli-service lint . --fix",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.5.0",
    "@vue/cli-plugin-babel": "^5.0.3",
    "@vue/cli-plugin-eslint": "^5.0.3",
    "@vue/cli-service": "^5.0.3",
    "vue-router": "^4.0.14",
    "eslint": "^8.11.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
